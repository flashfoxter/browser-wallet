{
  "name": "browser-wallet",
  "version": "1.0.0",
  "author": "https://gotiger.com",
  "scripts": {
    "build": "browserify -t [ babelify --presets [ es2015 react stage-1 ] --plugins transform-runtime] src/index.js -o app.js",
    "build:dist": "npm run clean && npm run mkdirs && npm run copyfiles",
    "mkdirs": "mkdir dist && cd dist && mkdir icons",
    "clean": "rimraf dist",
    "copyfiles": "cp -r icons/* dist/icons && cp app.js dist && cp popup.html dist && cp manifest.json dist && cp wallet-logo.png dist"
  },
  "dependencies": {
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "aes256": "^1.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^16.2.3",
    "chrome-https": "^2.0.1",
    "copy-text-to-clipboard": "^1.0.4",
    "ethereumjs-wallet": "^0.6.2",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "redux-undo": "^0.6.1",
    "truffle-hdwallet-provider": "0.0.6",
    "url": "^0.11.0",
    "web3": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "redux-devtools": "^3.1.1"
  }
}
